{
    "name": "paulhennell/twitter-account-info",
    "description": "A package to get basic account info like follower count number for a twitter account without requiring a twitter API login.",
    "keywords": [
        "paulhennell",
        "twitter-account-info"
    ],
    "homepage": "https://github.com/paulhennell/twitter-account-info",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Hennell",
            "email": "hennell@hennell.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.14",
        "php-http/message-factory": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "guzzlehttp/psr7": "^2.4",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/mock-client": "^1.5",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Paulhennell\\TwitterAccountInfo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paulhennell\\TwitterAccountInfo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
